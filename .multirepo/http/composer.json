{
    "name": "railt/http",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt HTTP component provides objects to build GraphQL requests from the client.",
    "keywords": [
        "http",
        "http response",
        "response",
        "http request",
        "batching",
        "parsing",
        "zend",
        "laravel",
        "symfony",
        "request",
        "graphql"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http"
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "railt/support": "~1.4.0|1.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "illuminate/http": "~5.5",
        "symfony/http-foundation": "~3.4|~4.0",
        "zendframework/zend-diactoros": "~1.7",
        "zendframework/zend-http": "~2.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Http\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "suggest": {
        "symfony/http-foundation": "(~3.4|~4.0) Symfony HTTP Kernel",
        "illuminate/http": "(~5.5) Laravel HTTP Kernel"
    }
}
