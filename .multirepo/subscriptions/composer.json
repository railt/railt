{
    "name": "railt/subscriptions",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework GraphQL Subscriptions implementation.",
    "keywords": [
        "websocket",
        "subscriptions",
        "graphql",
        "socket",
        "server",
        "async"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/subscriptions"
    },
    "require": {
        "php": ">=7.1.3",
        "railt/railt": "1.4.*|1.4.x-dev",
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Subscriptions\\": "tests/"
        }
    },
    "suggest": {
        "ext-swoole": "(~4.1) High-performance, scalable, concurrent WebSocket extension in PHP programming language",
        "cboden/ratchet": "(~0.4) PHP implementation of WebSocket protocol"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
