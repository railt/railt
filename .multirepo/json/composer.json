{
    "name": "railt/json",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "Simple library for working with json data",
    "keywords": [
        "json",
        "parsing",
        "read",
        "write",
        "decode",
        "encode",
        "json5"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "railt/io": "1.4.*|1.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        },
        "files": [
            "./src/Json/polyfill.php",
            "./src/Json/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "railt/lexer": "1.4.*|1.4.x-dev",
        "railt/parser": "1.4.*|1.4.x-dev",
        "railt/compiler": "1.4.*|1.4.x-dev",
        "justinrainbow/json-schema": "~5.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Json\\": "tests"
        }
    },
    "suggest": {
        "railt/lexer": "(1.4.*) JSON5 decoder support",
        "railt/parser": "(1.4.*) JSON5 decoder support",
        "justinrainbow/json-schema": "(~5.2) JSON validation support"
    },
    "extra": {
        "railt": {
            "commands": [
                "Railt\\Json\\Console\\Json5CompileCommand"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
