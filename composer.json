{
    "name": "railt/railt",
    "type": "library",
    "description": "The Railt Framework.",
    "license": "MIT",
    "keywords": [
        "graphql",
        "sdl",
        "ast",
        "framework",
        "reflection",
        "railt",
        "application"
    ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "bin": [
        "bin/railt"
    ],
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "ext-json": "*",
        "psr/container": "~1.0",
        "psr/log": "~1.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0",
        "illuminate/support": "~5.5",
        "webonyx/graphql-php": "~0.10",
        "symfony/console": "3.*|4.*",
        "symfony/event-dispatcher": "3.*|4.*",
        "railt/compiler": "1.2.*|dev-master",
        "railt/io": "1.2.*|dev-master",
        "railt/http": "1.2.*|dev-master",
        "railt/container": "1.2.*|dev-master",
        "railt/storage": "1.2.*|dev-master"
    },
    "replace": {
        "railt/sdl": "self.version",
        "railt/reflection": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "twig/twig": "~2.0",
        "zendframework/zend-code": "~3.0",
        "symfony/finder": "~4.0",
        "cache/filesystem-adapter": "~1.0",
        "symfony/var-dumper": "~4.0",
        "codedungeon/phpunit-result-printer": "~0.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\": "tests"
        }
    },
    "suggest": {
        "monolog/monolog": "(1.*) PSR-3 Logger Implementation"
    },
    "scripts": {
        "build": [
            "@php bin/railt compiler:build"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "source"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
