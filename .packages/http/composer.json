{
    "name": "railt/http",
    "type": "library",
    "description": "The Railt HTTP component provides objects to build GraphQL requests from the client.",
    "version": "1.2.0",
    "license": "MIT",
    "keywords": [
        "http",
        "http response",
        "response",
        "http request",
        "request",
        "graphql"
    ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "illuminate/support": "~5.5",
        "symfony/var-dumper": "~3.3|~4.0",
        "illuminate/http": "~5.5",
        "symfony/http-foundation": "~3.3|~4.0",
        "zendframework/zend-diactoros": "~1.7",
        "zendframework/zend-http": "~2.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Http\\": "tests"
        }
    },
    "suggest": {
        "symfony/http-foundation": "Symfony HTTP Kernel (>=3.3)",
        "illuminate/http": "Laravel HTTP Kernel (>=5.5)"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
