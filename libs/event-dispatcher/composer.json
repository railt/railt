{
    "name": "railt/event-dispatcher",
    "description": "The Railt Event Dispatcher is a simple chaining memleak-free event system",
    "license": "MIT",
    "keywords": ["events", "event", "listener", "dispatcher"],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/event-dispather"
    },
    "require": {
        "php": "^8.1",
        "psr/event-dispatcher": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\EventDispatcher\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "vimeo/psalm": "^5.15"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\EventDispatcher\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
