{
    "name": "railt/http",
    "license": "MIT",
    "keywords": ["http", "response", "request", "batching", "parsing", "graphql"],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http"
    },
    "require": {
        "php": "^8.1",
        "railt/http-contracts": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Http\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
