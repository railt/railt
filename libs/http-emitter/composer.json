{
    "name": "railt/http-emitter",
    "description": "GraphQL HTTP Emitter",
    "license": "MIT",
    "keywords": [
        "http",
        "response",
        "emitter"
    ],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http-emitter"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "railt/http-factory": "^2.0",
        "railt/http-contracts": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\Emitter\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Http\\Emitter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
