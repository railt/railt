{
    "name": "railt/type-system",
    "description": "Interoperable GraphQL Type System",
    "license": "MIT",
    "keywords": ["graphql", "language", "sdl", "type system", "interop", "abstraction"],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/type-system"
    },
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Railt\\TypeSystem\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\TypeSystem\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
