{
    "name": "railt/type-system",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "graphql",
        "language",
        "sdl",
        "type system",
        "interop",
        "abstraction"
    ],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/type-system"
    },
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Railt\\TypeSystem\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "vimeo/psalm": "^5.15"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\TypeSystem\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
