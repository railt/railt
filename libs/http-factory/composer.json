{
    "name": "railt/http-factory",
    "description": "GraphQL HTTP Factories",
    "license": "MIT",
    "keywords": [
        "http",
        "response",
        "request",
        "batching",
        "parsing",
        "graphql",
        "factory"
    ],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http-factory"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "railt/http": "^2.0",
        "railt/http-contracts": "^2.0",
        "railt/http-factory-contracts": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\Factory\\": "src"
        }
    },
    "require-dev": {
        "psr/http-message": "^1.0|^2.0",
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.14"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Http\\Factory\\Tests\\": "tests"
        }
    },
    "suggest": {
        "psr/http-message": "(^1.0|^2.0) To be able to create GraphQL requests from PSR-7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
