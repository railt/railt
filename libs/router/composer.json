{
    "name": "railt/router",
    "license": "MIT",
    "keywords": ["graphql", "router"],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/router"
    },
    "require": {
        "php": "^8.1",
        "railt/http-contracts": "^2.0",
        "psr/container": "^1.0|^2.0",
        "psr/event-dispatcher": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Router\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Router\\Tests\\": "tests"
        }
    },
    "suggest": {
        "railt/foundation": "(^2.0) For extension loading"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
