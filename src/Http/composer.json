{
    "name": "railt/http",
    "type": "library",
    "description": "The Railt HTTP component provides objects to build GraphQL requests from the client.",
    "license": "MIT",
    "keywords": [ "http", "http response", "response", "http request", "request", "graphql" ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\": ""
        }
    },
    "suggest": {
        "symfony/http-foundation": "Required to use the Symfony Request driver (~2.8|3.*|4.*)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
