{
    "name": "railt/http-factory",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework implementation of GraphQL HTTP factory component library",
    "keywords": [
        "http",
        "factory",
        "parsing"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http-factory"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "railt/contracts": "^2.0",
        "psr/http-message": "~1.0",
        "railt/http": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\HttpFactory\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\HttpFactory\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "@php vendor/phpunit/phpunit/phpunit"
    },
    "extra": {
        "railt": {
            "extensions": [
                "Railt\\HttpFactory\\HttpFactoryServiceExtension"
            ]
        },
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
