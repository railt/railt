{
    "name": "railt/container",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework dependency injection container",
    "keywords": [
        "di",
        "container",
        "dependency injection",
        "ioc"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/container"
    },
    "require": {
        "php": ">=7.4",
        "railt/contracts": "^2.0",
        "railt/dumper": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Container\\": "src"
        }
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "vimeo/psalm": "^3.5",
        "phpunit/phpunit": "^8.3",
        "illuminate/container": "~5.5|~6.0",
        "symfony/dependency-injection": "~3.4|~4.0|~5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Container\\Tests\\Unit\\": "tests/unit",
            "Railt\\Container\\Tests\\Feature\\": "tests/feature"
        },
        "files": [
            "tests/unit/Mock/global_test_function.php",
            "tests/unit/Mock/test_function.php"
        ]
    },
    "provide": {
        "psr/container-implementation": "1.0",
        "psr/container": "1.0"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
