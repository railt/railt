{
    "name": "railt/introspection",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework implementation of GraphQL Introspection Client",
    "keywords": [
        "http",
        "factory",
        "parsing"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/introspection"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "phplrt/parser": "^2.3",
        "phplrt/lexer": "^2.3",
        "phplrt/position": "^2.3",
        "phplrt/grammar": "^2.3",
        "railt/type-system": "^2.0",
        "psr/http-client": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Introspection\\": "src"
        }
    },
    "require-dev": {
        "symfony/console": "~3.4|~4.0|~5.0",
        "phplrt/compiler": "^2.3",
        "phpunit/phpunit": "^8.3",
        "cache/filesystem-adapter": "~1.0",
        "zendframework/zend-diactoros": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Introspection\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "split": "monorepo-builder split",
        "merge": "monorepo-builder merge"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
