{
    "name": "railt/http",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework implementation of GraphQL HTTP component library",
    "keywords": [
        "http",
        "http response",
        "response",
        "http request",
        "batching",
        "parsing",
        "zend",
        "laravel",
        "symfony",
        "request",
        "graphql"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "phplrt/source": "~2.2",
        "phplrt/position": "~2.2",
        "railt/dumper": "^2.0",
        "railt/container": "^2.0",
        "ramsey/collection": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\": "src/"
        }
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "vimeo/psalm": "^3.5",
        "phpunit/phpunit": "^8.3",
        "illuminate/http": "~5.5|~6.0",
        "symfony/http-foundation": "~3.4|~4.0|~5.0",
        "zendframework/zend-diactoros": "~1.7|~2.0",
        "zendframework/zend-http": "~2.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Http\\Tests\\Unit\\": "tests/unit",
            "Railt\\Http\\Tests\\Feature\\": "tests/feature"
        }
    },
    "suggest": {
        "symfony/http-foundation": "(~3.4|~4.0|~5.0) Symfony HTTP Kernel",
        "illuminate/http": "(~5.5|~6.0) Laravel HTTP Kernel",
        "zendframework/zend-diactoros": "(~1.7|~2.0) Zend Http PSR-7 Kernel",
        "zendframework/zend-http": "(~2.8) Zend Http Kernel"
    },
    "extra": {
        "railt": {
            "extensions": [
                "Railt\\Http\\HttpExtension"
            ]
        },
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
